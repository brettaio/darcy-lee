{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "websites",
  "projects": {
    "enzo-concrete": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:resolver": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "websites/enzo-concrete",
      "sourceRoot": "websites/enzo-concrete/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/enzo-concrete",
            "index": "websites/enzo-concrete/src/index.html",
            "browser": "websites/enzo-concrete/src/main.ts",
            "polyfills": [],
            "tsConfig": "websites/enzo-concrete/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "websites/enzo-concrete/public"
              }
            ],
            "styles": [
              "websites/enzo-concrete/src/styles.css"
            ],
            "scripts": [],
            "server": "websites/enzo-concrete/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "websites/enzo-concrete/src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "enzo-concrete:build:production"
            },
            "development": {
              "buildTarget": "enzo-concrete:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        }
      }
    },
    "component": {
      "projectType": "library",
      "root": "component",
      "sourceRoot": "component/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "component/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "component/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "component/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "component/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "page": {
      "projectType": "library",
      "root": "page",
      "sourceRoot": "page/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "page/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "page/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "page/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "page/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "icon": {
      "projectType": "library",
      "root": "icon",
      "sourceRoot": "icon/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "icon/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "icon/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "icon/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "icon/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  }
}
